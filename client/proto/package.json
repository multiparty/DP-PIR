{
  "name": "drivacy-protobuf-client",
  "dependencies": {
    "browserify": "^16.5.2",
    "google-protobuf": "^3.13.0"
  },
  "scripts": {
    "proto": "protoc --js_out=import_style=commonjs,binary:. -I../../drivacy/types config.proto messages.proto",
    "build": "./node_modules/.bin/browserify --s drivacy index.js  -o drivacy.js",
    "browserify": "npm run proto && npm run build"
  }
}
